{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "strict": true,
    "moduleResolution": "node",
    "module": "ESNext",
    "lib": ["ESNext"],
    "target": "ESNext",
    "isolatedModules": true,
    "outDir": "./dist",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "allowJs": true,
    "checkJs": true,
    "jsx": "preserve"
  }
}
